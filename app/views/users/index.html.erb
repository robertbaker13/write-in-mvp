
<div class='home-left-column debug'>
</div>




<div class='home-middle-column debug'>

<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Twitter</th>
      <th>Token</th>
      <th>Expires</th>
      <th>District</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.twitter_id %></td>
        <td><%= user.token %></td>
        <td><%= user.expires %></td>
        <td><%= user.district %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>

<div class='home-right-column debug'>
  </div>

